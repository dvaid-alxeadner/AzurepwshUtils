{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "name": {
            "value": "mysqlflex202510",
            "metadata": {
                "description": "Mysql flexible server name."
            }
        },
        "subnet": {
            "value": "/subscriptions/<SubscriptionID>/resourceGroups/<ResourceGroupName>/providers/Microsoft.Network/virtualNetworks/<VNETName>/subnets/<Subnet Name>",
            "metadata": {
                "description": "id for the subnet which Mysql will be delegated"
            }
        },
        "userAssignedIdentities_externalid":
        {
            "value": "/subscriptions/<SubscriptionID>/resourceGroups/<ResourceGroupName>/providers/Microsoft.ManagedIdentity/userAssignedIdentities/<Managed Identity Name>",
            "metadata": {
                "description": "id for the User Managed Identity for Mysql"
            }
        },
        "sku": {
            "value": "Standard_B1ms",
            "metadata": {
                "description": "Stock Keeping Unit"
            }
        },
        "tier": {
            "value": "Burstable",
            "metadata": {
                "description": "Tier for the MySQL database"
            }
        },
        "administratormail": {
            "value": "CONTOSO@MICROSOFT.NET",
            "metadata": {
                "description": "UPN from Azure Active Directory who has administrative responsibilities over the database"
            }
        },
        "adminpassword": {
            "value": null,
            "metadata": {
                "description": "Admin Password"
            }
        },
        "administratorobjectid":{
            "value": "<aabbccdd-ffff-aabb-ccdd-aabbccddeeff>",
            "metadata": {
                "description": "Object ID from Microsoft Entra ID who has administrative responsibilities over the database"
            }
        },
        "administratorIP":{
            "value": "10.254.254.200",
            "metadata": {
                "description": "IP address allowed for administrative responsibilities over the database"
            }
        },
        "storageGB": {
            "value": 20,
            "metadata": {
                "description": "Max size of the database expressed in Gigabytes"
            }
        },
        "iops": {
            "value": 360,
            "metadata": {
                "description": "Quantity of input/output operations"
            }
        },
        "environment_tag":
        {
            "value":"TEST",
            "metadata": {
                "description": "Tag to describe the enviroment"
            }
        },
        "oi_tag":
        {
            "value":"10000111",
            "metadata": {
                "description": "Tag for budget classification"
            }
        }
    }
}
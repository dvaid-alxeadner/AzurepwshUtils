{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "subscriptionId": {
            "value": "<aabbccdd-ffff-aabb-ccdd-aabbccddeeff>",
            "metadata": {
                "description": "id of the Azure Subscription"
            }
        },
        "resourceGroup":{
            "value": "<Resource Group>",
            "metadata":{
                "description": "Resource group name."
            }
        },
        "storageAccountName": {
            "value": "storagedic22",
            "metadata": {
                "description": "Storage account name."
            }
        },
        "location": {
            "value": "eastus2",
            "metadata": {
                "description": "Location of the storage account."
            }
        },
        "accountType": {
            "value": "Standard_LRS"
        },
        "kind": {
            "value": "StorageV2"
        },
        "accessTier": {
            "value": "Hot"
        },
        "defaultOAuth": {
            "value": false
        },
        "allowedCopyScope": {
            "value": "AAD"
        },
        "dnsEndpointType": {
            "value": "Standard"
        },
        "keySource": {
            "value": "Microsoft.Storage"
        },
        "isContainerRestoreEnabled": {
            "value": false
        },
        "isBlobSoftDeleteEnabled": {
            "value": false
        },
        "isContainerSoftDeleteEnabled": {
            "value": false
        },
        "changeFeed": {
            "value": false
        },
        "isVersioningEnabled": {
            "value": false
        },
        "isShareSoftDeleteEnabled": {
            "value": false
        },
        "privateEndpointName": {
            "value": "PE_STORAGEBLOB",
